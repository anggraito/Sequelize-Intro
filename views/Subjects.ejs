<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
  <title>Subject Page</title>
  </head>

  <body>
    <%- include layout/menu%>
  <center><h3>Subject data</h3><center><br>
  <br><br>
  <center>
  <table>
    <tr>
      <th>id</th>
      <th>Subject Name</th>
      <th>Teacher</th>
      <th>Action</th>
    </tr>
    <% data_subjects.forEach((subject, index )=>{%>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= subject.subject_name %></td>
      <td>
        <ol type="1">
        <% subject.full_name.forEach(names =>{%>
        <li> <%= names %> </li>
        <%})%>
        </ol>
      </td>
      <td><a href="/subjects/enrolledstudents/<%=subject.id%>">Enrolled Students</a></td>
    </tr>
      <%})%>
    </table>
  </center>
  </body>
</html>
