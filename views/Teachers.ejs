<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">

  <title>Teacher Page</title>
  </head>

  <body>
  <center><h3>Teachers data</h3><center><br>
  <br><br>
  <center>
  <table>
    <tr>
      <th>id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan='2'></th>
    </tr>
    <% data_teachers.forEach(teacher =>{%>
    <tr>
      <td><%= teacher.id %></td>
      <td><%= teacher.first_name %></td>
      <td><%= teacher.last_name %></td>
      <td><%= teacher.email %></td>
    </tr>
      <%})%>
    </table>
  </center>
  </body>
</html>
